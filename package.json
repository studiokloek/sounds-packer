    "eslint-plugin-unicorn": "^47.0.0",
    "np": "^8.0.4",
    "rollup": "^3.29.5",
    "source-map-support": "^0.5.21"
  },
  "dependencies": {
    "camelcase": "^7.0.1",
    "chalk": "^5.4.1",
    "fs-extra": "^11.3.0",
    "get-audio-duration": "4.0.1",
    "get-value": "^3.0.1",
    "globby": "^13.2.2",
    "hasha": "^5.2.2",
    "log-symbols": "^5.1.0",
    "meow": "^12.0.1",
    "object.defaults": "^1.1.0",
    "ora": "^6.3.1",
    "pupa": "^3.1.0",
